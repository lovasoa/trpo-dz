{
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid     1 class ListViewer \{\par
    2   view(model) \{\par
    3     const tablename = model.apiname,\par
    4           array = model.results;\par
    5     const $button = $("<button>\'D1\'EE\'E7\'E4\'E0\'F2\'FC \'ED\'EE\'E2\'FB\'E9</button>")\par
    6                       .on("click", evt=>model.create());\par
    7     if (array.length === 0) return $button;\par
    8     const headers = Object.keys(array[0]).filter(col => col!=tablename+"_id");\par
    9     return $("<div>").append(\par
   10       $("<ul>").append(\par
   11         $.map(array, e => this.viewElem(model, headers, e)\par
   12       ))\par
   13     ).append($button);\par
   14   \}\par
   15 \par
   16   viewElem(model, headers, e) \{\par
   17     return $("<li>").append($.map(headers, h =>\par
   18       $("<label>")\par
   19           .text(h)\par
   20           .append(this.viewInput(model, e, h))\par
   21     )).append(\par
   22       $("<button>\'D3\'E4\'E0\'EB\'E8\'F2\'FC</button>")\par
   23         .on("click", evt=>model.delete(e))\par
   24     )\par
   25   \}\par
   26 \par
   27   viewInput(model, e, h) \{\par
   28     const handleEvt = evt => model.update(e,h,evt.target.value);\par
   29     let input = $("<input>")\par
   30                 .val(e[h])\par
   31                 .on("input", handleEvt);\par
   32     if (h.endsWith("_id")) \{\par
   33       input = $("<select>");\par
   34       input.change(handleEvt);\par
   35       const apiname = h.slice(0, -3);\par
   36       fetch(`../backend/api.php?api=$\{apiname\}&type=list`)\par
   37         .then(r => r.json())\par
   38         .then(r => input.append(\par
   39               $.map(r, op =>\par
   40                 $("<option>")\par
   41                   .val(op[h])\par
   42                   .attr("selected", op[h]==e[h])\par
   43                   .text(op[Object.keys(op).filter(x=>!x.endsWith("_id"))[0]]))\par
   44           ));\par
   45     \}\par
   46     if (h === "filename") \{\par
   47       let nameinput = input;\par
   48       input = $("<span>")\par
   49                   .append(nameinput)\par
   50                   .append(\par
   51                     $("<input type='file'>")\par
   52                       .on("change", evt => \{\par
   53                             const file = evt.target.files[0];\par
   54                             const filename = file.name;\par
   55                             nameinput.val(filename);\par
   56                             model.update(e,h,filename,\par
   57                                           \{method:"POST", "body":file\});\par
   58                           \})\par
   59                   )\par
   60     \}\par
   61     return input;\par
   62   \}\par
   63 \}\par
   64 \par
   65 \par
   66 class TabularData \{\par
   67   constructor(viewer) \{\par
   68     this.viewer = viewer;\par
   69   \}\par
   70 \}\par
   71 \par
   72 class Request extends TabularData \{\par
   73   constructor(viewer, apiname, viewEl) \{\par
   74     super(viewer);\par
   75     this.apiname = apiname;\par
   76     this.viewEl = viewEl;\par
   77   \}\par
   78   read() \{\par
   79     return this.fetch(\{type:"list"\})\par
   80               .then(r => \{return this.results = r\})\par
   81               .then(() => this.view());\par
   82   \}\par
   83   update(data, column, value, options) \{\par
   84     const type = "update";\par
   85     return this.fetch(\{\par
   86                         type,\par
   87                         data,\par
   88                         column,\par
   89                         value\par
   90                     \}, options);\par
   91   \}\par
   92   create() \{\par
   93     this.fetch(\{type:"create"\}).then(()=>this.read());\par
   94   \}\par
   95   delete(data) \{\par
   96     this.fetch(\{type:"delete",data\}).then(()=>this.read());\par
   97   \}\par
   98   fetch(params, options) \{\par
   99     var qs = new URLSearchParams("");\par
  100     qs.set("api", this.apiname);\par
  101     for (var p in params)\par
  102       qs.set(p, typeof(params[p])==="string"?params[p]:JSON.stringify(params[p]));\par
  103     let fetched = fetch("../backend/api.php?" + qs, options)\par
  104                       .then(x => x.json());\par
  105     return fetched;\par
  106   \}\par
  107   view(data) \{\par
  108     this.viewEl.empty();\par
  109     this.viewEl.append(this.viewer.view(this));\par
  110     return data;\par
  111   \}\par
  112 \}\par
  113 \par
  114 \par
  115 class Main \{\par
  116   constructor() \{\par
  117     this.resEl = $("#result");\par
  118     this.tables = ["professor", "discipline", "material"];\par
  119     $("#apis").append($.map(this.tables, t=>\par
  120         $("<li>")\par
  121           .attr("id",t)\par
  122           .text(t)\par
  123           .on("click", ()=> this.do_request(t))\par
  124     ));\par
  125     this.do_request(this.tables[0]);\par
  126   \}\par
  127 \par
  128   do_request(apiname) \{\par
  129     const viewer = new ListViewer();\par
  130     this.current_request = new Request(viewer, apiname, this.resEl);\par
  131     this.current_request.read();\par
  132   \}\par
  133 \}\par
  134 \par
  135 var main = new Main;\par
}
